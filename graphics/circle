 * @author sjcet
 */
import java.util.Scanner;
public class circle {
    public void cirarea(){
        Scanner obj=new Scanner(System.in);
        System.out.println("enter radius of circle");
        int r=obj.nextInt();
        double res4=r*r*Math.PI;
        System.out.println("area of circle:"+res4);
    }
    
}

